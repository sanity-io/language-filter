{
  "name": "@sanity/language-filter",
  "version": "3.0.0-v3-studio.8",
  "description": "A Sanity plugin that supports filtering localized fields by language",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./lib/index.js",
  "module": "./lib/index.esm.js",
  "types": "./lib/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/src/index.d.ts",
      "source": "./src/index.ts",
      "import": "./lib/index.esm.js",
      "require": "./lib/index.js",
      "default": "./lib/index.esm.js"
    }
  },
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint .",
    "prebuild": "npm run clean && plugin-kit verify-package --silent && pkg-utils",
    "build": "pkg-utils build",
    "watch": "pkg-utils watch",
    "test": "jest",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/language-filter.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@sanity/icons": "^1.3.4",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.0.0-beta.31",
    "@sanity/util": "3.0.0-rc.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@sanity/pkg-utils": "^1.16.2",
    "@sanity/plugin-kit": "^2.0.9",
    "@sanity/semantic-release-preset": "^2.0.2",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "sanity": "dev-preview || 3.0.0-rc.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "sanity": "dev-preview || 3.0.0-rc.0"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/language-filter/issues"
  },
  "homepage": "https://github.com/sanity-io/language-filter#readme",
  "sanityPlugin": {
    "verifyPackage": {
      "babelConfig": false
    }
  }
}
