{
  "name": "@sanity/language-filter",
  "version": "1.0.0",
  "description": "A Sanity plugin that supports filtering localized fields by language",
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint .",
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "test": "jest",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/language-filter.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@sanity/icons": "^1.3.4",
    "@sanity/incompatible-plugin": "^1.0.0",
    "@sanity/ui": "^0.38.0",
    "@sanity/util": "3.0.0-dev-preview.15"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@sanity/plugin-kit": "^1.0.1",
    "@sanity/semantic-release-preset": "^2.0.1",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "react": "^17.0.0 || ^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-dev-preview.15",
    "ts-jest": "^28.0.8",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "sanity": "dev-preview"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/language-filter/issues"
  },
  "homepage": "https://github.com/sanity-io/language-filter#readme",
  "sanityPlugin": {
    "verifyPackage": {
      "babelConfig": false
    }
  }
}
